{
    "name": "ibnudirsan/handler-sanctum",
    "keywords": [
        "sanctum",
        "api",
        "framework",
        "laravel"
      ],
    "description": "Laravel Sanctum API Handler.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ibnudirsan\\HandlerSanctum\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag={sanctum-config-ibnudirsan}--force"
         ],
         "post-create-project-cmd": [
             "@php artisan config:publish --ansi"
         ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibnudirsan\\HandlerSanctum\\ServiceProvider\\HandlerSanctum"
            ],
            "scripts": {
                 "post-create-project-cmd": [
                     "@php artisan config:publish --ansi"
                 ]
            }
        }
    },
    "authors": [
        {
            "name": "ibnudirsan",
            "email": "ibnudirsan@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9.0"
    }
}
